  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "bootstrap": "^5.3.2",
    "date-fns": "^3.1.0",
    "express": "^4.18.2",
    "helmet": "^6.2.0",
    "history": "^5.3.0",
    "http-proxy-middleware": "^2.0.6",
    "jss": "^10.9.2",
    "jss-preset-default": "^10.9.2",
    "less": "^3.13.1",
    "less-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-jss": "^10.10.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^5.3.4",
    "react-test-renderer": "^18.2.0",
    "react-to-print": "^2.14.15",
    "recharts": "^2.10.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.3",
    "redux-thunk": "^2.4.2",
    "regenerator-runtime": "^0.13.11",
    "sass": "^1.69.5",
    "underscore": "^1.13.6",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-react": "^7.23.3",
    "@babel/traverse": "^7.23.4",
    "@types/jss": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jss": "^1.0.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^1.19.1",
    "react-scripts": "^5.0.1",
    "redux-mock-store": "^1.5.4"
  },
  "scripts": {
    "audit": "npm audit --production",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test.watch": "jest --watch",
    "test.update": "jest --updateSnapshot",
    "test.coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint \"**/*.{js,jsx}\"",
    "lint-fix": "eslint \"**/*.{js,jsx}\" --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "snapshotSerializers": [],
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./test-setup.js"
    ],
    "collectCoverage": false,
    "coverageDirectory": "./coveragereport",
    "coverageReporters": [
      "cobertura",
      "lcov",
      "HTML",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/index.js",
      "!src/App.js",
      "!src/index.js",
      "!<rootDir>/node_modules/",
      "!<rootDir>/assets-transformer.js",
      "!src/reportWebVitals.js"
    ],
    "modulePathIgnorePatterns": [],
    "moduleNameMapper": {
      "^Components(.*)$": "<rootDir>/src/components$1",
      "^Styles(.*)$": "<rootDir>/src/styles$1",
      "^Data(.*)$": "<rootDir>/src/data$1",
      "^Assets(.*)$": "<rootDir>/src/assets$1",
      "^Classes(.*)$": "<rootDir>/src/classes$1",
      "^Redux(.*)$": "<rootDir>/src/redux$1",
      "^Utilities(.*)$": "<rootDir>/src/utilities$1",
      "\\.(css|less|sass|scss)$": "<rootDir>/assets-transformer.js"
    },
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assets-transformer.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
